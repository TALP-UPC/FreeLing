1 - espec-mp_$$ (sadv(més),parti-mp) - top_right RELABEL sadv-sup:-
1 - espec-fp_$$ (sadv(més),parti-fp) - top_right RELABEL sadv-sup:-

1 - espec-ms_$$ (sadv(menys),parti-ms) - top_right RELABEL sadv-sup:- % ((menys) adormida)
1 - espec-fs_$$ (sadv(menys),parti-fs) - top_right RELABEL sadv-sup:-
1 - espec-mp_$$ (sadv(menys),parti-mp) - top_right RELABEL sadv-sup:-
1 - espec-fp_$$ (sadv(menys),parti-fp) - top_right RELABEL sadv-sup:-

% ADJECTIUS ESPECIFICATS i: ADVERBIS (La meva mare esta [bastant] adormida)

1 - - (sadv,s-adj) - top_right RELABEL -:- % ((bastant) xerraire)

1 - - (sadv,parti-ms) - top_right RELABEL -:- % ((quasi) adormit)
1 - - (sadv,parti-fs) - top_right RELABEL -:-
1 - - (sadv,parti-mp) - top_right RELABEL -:-
1 - - (sadv,parti-fp) - top_right RELABEL -:-


% ADJECTIUS ESPECIFICATS ii: DETERMINANTS (La meva mare es [la] mes xerraire) %
2 - - (espec-ms,s-adj) - top_right RELABEL -:- % ((el) mes xerraire)
2 - - (espec-fs,s-adj) - top_right RELABEL -:-
2 - - (espec-mp,s-adj) - top_right RELABEL -:-
2 - - (espec-fp,s-adj) - top_right RELABEL -:-


